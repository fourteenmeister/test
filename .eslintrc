{
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "extends": "airbnb",
  "plugins": [],
  "rules": {
    "linebreak-style": ["error", "windows"],
    "max-len": ["error", 120]
  }
}
